<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<!-- Do use PrettyFaces ONLY for navigation -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>driverListView</from-outcome>
			<to-view-id>/private/driver/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>truckListView</from-outcome>
			<to-view-id>/private/truck/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>shipmentListView</from-outcome>
			<to-view-id>/private/shipment/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>unloadingListView</from-outcome>
			<to-view-id>/private/unloading/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>customerListView</from-outcome>
			<to-view-id>/private/customer/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>productListView</from-outcome>
			<to-view-id>/private/product/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
	</navigation-rule>

</faces-config>
