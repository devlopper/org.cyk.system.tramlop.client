<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">

	<!-- Do use PrettyFaces ONLY for navigation -->

	<navigation-rule>
		<from-view-id>*</from-view-id>
		
		<navigation-case>
			<from-outcome>agreementCreateView</from-outcome>
			<to-view-id>/private/agreement/create.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>agreementListView</from-outcome>
			<to-view-id>/private/agreement/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>agreementReadView</from-outcome>
			<to-view-id>/private/agreement/read.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>deliveryBoardView</from-outcome>
			<to-view-id>/private/delivery/board.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>deliveryWeighBeforeChargeView</from-outcome>
			<to-view-id>/private/delivery/weighbeforecharge.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>deliveryLoadView</from-outcome>
			<to-view-id>/private/delivery/load.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>deliveryWeighView</from-outcome>
			<to-view-id>/private/delivery/weigh.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>deliveryReceiveView</from-outcome>
			<to-view-id>/private/delivery/receive.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>deliveryListView</from-outcome>
			<to-view-id>/private/delivery/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>deliveryReadView</from-outcome>
			<to-view-id>/private/delivery/read.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
		<navigation-case>
			<from-outcome>productListView</from-outcome>
			<to-view-id>/private/product/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>truckListView</from-outcome>
			<to-view-id>/private/truck/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>placeListView</from-outcome>
			<to-view-id>/private/place/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>pathListView</from-outcome>
			<to-view-id>/private/path/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>driverListView</from-outcome>
			<to-view-id>/private/driver/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>customerListView</from-outcome>
			<to-view-id>/private/customer/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		<navigation-case>
			<from-outcome>taskListView</from-outcome>
			<to-view-id>/private/task/list.jsf</to-view-id>
			<redirect />
		</navigation-case>
		
	</navigation-rule>

</faces-config>
